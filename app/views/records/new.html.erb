<% content_for :head do %>
  <%= stylesheet_link_tag 'static_pages' %>
  <%= stylesheet_link_tag 'records' %>
  <%= javascript_include_tag 'records' %>
<% end %>

<% provide(:page_title, "New Record") %>

<div class="small-banner-image-container students-image">
  <%= image_tag("students.jpg") %> 
</div>

<div class="container">
  <div class="form-container">
    <h4 class="center-text">Contribute</h4>

    <div class="preface-text">Our stories are personal, but they are not unique. Join us as we begin to document and create a space for our voices in tomorrow's Yale. This is the Yale we choose. Our voices. Our stories. Our Yale.</div>

    <%= render 'form' %>
    
  </div>
</div>
